  <div class="container">
      <div class="header">
        <nav>
          <ul class="nav nav-pills pull-right">
            <li role="presentation"><a href="#" onclick="window.open('http://www.facebook.com/sbssaf','new'); return false"><img src="/images/facebook_logo.png"/></a></li>
            <li role="presentation"><a href="#" onclick="window.open('http://twitter.com/SBSSAF','new'); return false"><img src="/images/twitter_logo.png"/></a></li>
          </ul>
        </nav>
        <h3 class="text-muted"><a href="http://www.sbs.co.kr"><img src="/images/white_logo.png" style="padding-top:10px;"/></a></h3>
      </div>

      <div class="jumbotron">
        <a href="/manage/logout">
          <h3>로그아웃</h3>
        </a>
        <h2>프로모션 목록</h2>
        <br>
        <%Promotion.all.each do |promotion|%>
          <a href="/events/result/<%=promotion.id%>"><p><%=promotion.title%></p></a>
        <%end%>
      </div>
      <div class="row marketing">
        <div class="col-lg-12">
          <h2>프로모션 생성</h2><br>
          <form role="form" action="/events/new" method="post">
            <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />
            <div class="form-group">
              <h3>프로모션 제목</h3>
              <input type="text" class="form-control" placeholder="제목" required style="width:150px;" name="title">
              <h3>프로모션 설명</h3>
              <textarea class="form-control" rows="3" name="content" placeholder ="설명"></textarea>
              <h3>페북의 경우 댓글 달릴 포스트 주소</h3>
              <input type="text" name ="post_url" value="https://" class="form-control">
              <h3>프로모션 안내 이미지 URL</h3>
              <input type="text" name ="img_url" value="http://" class="form-control">
            </div>
            <br>
            <input type="submit" class="btn btn-info" value="생성">
          </form>

        </div>
      </div>

      <footer class="footer">
        <p>© SBS&SBS Contents Hub.</p>
      </footer>

    </div> <!-- /container -->
